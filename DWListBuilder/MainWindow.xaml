<Window x:Class="DWListBuilder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:DWListBuilder.View.UserControls"
    xmlns:local="clr-namespace:DWListBuilder"
        mc:Ignorable="d"
        Title="Dystopian Wars List Builder" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="Lists">
                
            </TabItem>
            <TabItem Header="Orbats">
                <uc:OrbatEditorUC DataContext="{Binding OrbatEditorVM}">
                </uc:OrbatEditorUC>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
